	double formula (double x, int n){
		double sum = 1;
		for (int i = 1; i <= n; i++){
			sum=(pow (x, i)/factorial(i));
		}
		return 10; //linea 20 y 28 bot sure what happen si les pongo la linea  (prob solo en java funciona)
		
	}
		
		int factorial(int x){
			int y = 0;
			for (int i = 1;i<=x;i++){
				y=i+1;
			}
			return ;
		}
