#include <stdio.h>
 int main(){
 int c1,f1,c2,f2,x,y,res,i;
 printf("\nFilas de la primera matriz: ");
 scanf("%d",&f1);
 printf("\nColumnas de la primera matriz: ");
 scanf("%d",&c1);
 printf("\nFilas de la segunda matriz: ");
 scanf("%d",&f2);
 printf("\nColumnas de la segunda matriz: ");
 scanf("%d",&c2);
 int mtz1[f1][c1], mtz2[f2][c2], mtz3[50][50];
 if (f1==c2){
    printf("\nPrimer matriz");
     for(x=0;x<f1;x++)
      for (y=0;y<c1;y++){

       printf("\nIngrese la posicion [%d][%d]:",x,y);
       scanf("%d",&mtz1[x][y]);
      }

     printf("\nSegunda matriz");
     for(x=0;x<f2;x++)
      for (y=0;y<c2;y++){
       printf("\nIngrese la posicion [%d][%d]:",x,y);
       scanf("%d",&mtz2[x][y]);
      }

//impresion de matrices
     printf("Primer matriz");
       for(x=0;x<f1;x++)
      {
       printf("\n");
       for(y=0;y<c1;y++)
       printf("%d ",mtz1[x][y]);
      }
     printf("\nSegunda matriz");
       for(x=0;x<f2;x++)
      {
       printf("\n");
       for(y=0;y<c2;y++)
       printf("%d ",mtz2[x][y]);
      }
 //MULTIPLICACION
 for(x=0;x<f1;x++)
  for(y=0;y<c2;y++)
  {
   for(res=0,i=0;i<c1;i++)
    res=res+mtz1[x][i]*mtz2[i][y];

   mtz3[x][y]=res;
  }
//impresion de matriz
 printf("\nResultado de multiplicaciÃ³n");
  for(x=0;x<f1;x++)
  {
   printf("\n");
   for(y=0;y<c2;y++)
   printf("%d ",mtz3[x][y]);
  }                  }
     else
        printf("tas meco");
     return 0;
     }
