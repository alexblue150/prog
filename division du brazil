#include<stdio.h>
#include<math.h>
int main(){
int n,d,i=1;
printf("escribe el numerador \n");
scanf("%d",&n);
printf("escribe el denominador \n");
scanf("%d",&d);
if (d==0){
    printf("infinito");}
else if (n=0){
        printf("0");}
   else if(n=d){
            printf("1");}
       else if (n<d){
                printf("0");}
               else while (n>=0);{
                    n=n-d;
                    i++;
                     }
                        i=i-1;
                        printf("&d",i);
                        return 0;
                                        }
